 node{
		stage('Build') {
			steps {
				echo 'hello'
			}
		}
		stage('Migrations') {
			steps {
				echo 'migration'
			}
		}
		stage('envsetup') {
			steps {
				echo 'envsetup'
			}
		}
		stage('nodeadded') {
			steps {
				echo 'node added!'
			}
		}
		stage('Deploy') {
			steps {
				echo 'world'
			}
		}
 }