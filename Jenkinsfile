node{
		stage('Build') {
			echo 'hello'
		}
		stage('Migrations') {
			echo 'migration'
		}
		stage('envsetup') {
			echo 'envsetup'
		}
		stage('newnode') {
				echo 'with node'
		}
		stage('newtestnode') {
			echo 'new test node'
		}
		stage('newtestnodeadded') {
			echo 'new test node'
		}
		stage('executetestscript') {
			bat 'python D:/jenkins-tutorial/test_py_script.py'
		}
		stage('Deploy') {
				echo 'world'
		}
 }