node('unixnode') {
		stage('Build') {
			steps {
				echo 'hello'
			}
		}
		stage('Migrations') {
			steps {
				echo 'migration'
			}
		}
		stage('envsetup') {
			steps {
				echo 'envsetup'
			}
		}
		stage('newnode') {
			steps {
				echo 'with node'
			}
		}
		stage('Deploy') {
			steps {
				echo 'world'
			}
		}
 }